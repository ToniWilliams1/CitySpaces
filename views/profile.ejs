<%- include('partials/header') -%>
  <h2>Welcome, <%=user.userName %>!</h2>
  <div class="container">
  <div class="row mt-5">
     
      <div class="col-6">
        <h2 class="text-5xl font-bold">My Wishlist: <%=user.watching.length %></div>
        <% user.watching.forEach(data => { %>
          <div class="card w-96  ">
            <div class="card-body">
              <form
              class="col-1"
              method="GET"
            >
              <button class="trash-btn fa-solid fa-trash" type="submit"></button>
            </form>
              <h2 class="card-title"> <em><%= %></h2>
                  <p class="card-title"> <em><%= user.watching.data %></p>
                  <div class="card-actions justify-end">
                  </div>
            </div>
          </div>
        <% }) %>
        <ul class="row list-unstyled">
        </ul>
        <div class="row justify-content-center mt-5">
          <a class="btn" href="/events">Add A Space <i class="fa-solid fa-plus"></i> </a>
        </div>  
      
      
      </div>  
    </div>
  </div>
</div>
<%- include('partials/footer') -%>